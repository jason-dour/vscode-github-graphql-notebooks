{
	"name": "github-graphql-nb",
	"displayName": "GitHub GraphQL Notebooks",
	"publisher": "joyceerhl",
	"description": "Interactively run GitHub GraphQL queries and mutations in VS Code",
	"repository": {
		"url": "https://github.com/joyceerhl/vscode-github-graphql-notebooks"
	},
	"version": "0.0.6",
	"license": "MIT",
	"extensionDependencies": ["rebornix.vscode-code-renderer"],
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Notebooks"
	],
	"keywords": [
		"GitHub",
		"GraphQL"
	],
	"activationEvents": [
		"onNotebook:github-graphql-nb",
		"onCommand:github-graphql-nb.createnew"
	],
	"browser": "./dist/browser/extension",
	"main": "./dist/extension",
	"contributes": {
		"commands": [
			{
				"command": "github-graphql-nb.createnew",
				"title": "Create New GitHub GraphQL Notebook",
				"shortTitle": "GitHub GraphQL Notebook",
				"category": "GitHub GraphQL"
			}
		],
		"languages": [
			{
			  "id": "graphql",
			  "aliases": [
				"GraphQL",
				"graphql"
			  ],
			  "configuration": "./language/language-configuration.json"
			}
		  ],
		  "grammars": [
			{
			  "language": "graphql",
			  "path": "./grammars/graphql.json",
			  "scopeName": "source.graphql"
			}
		],
		"menus": {
			"file/newFile": [
				{
					"command": "github-graphql-nb.createnew"
				}
			]
		},
		"notebooks": [
			{
				"type": "github-graphql-nb",
				"displayName": "GitHub GraphQL Notebook",
				"priority": "default",
				"selector": [
					{
						"filenamePattern": "*.github-graphql-nb"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"serve": "npx serve --cors -l 5000",
		"tunnel": "npx localtunnel -p 5000"
	},
	"dependencies": {
		"@octokit/core": "3.5.1",
		"cross-fetch": "3.0.6",
		"node-fetch": "3.0.0"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"ts-loader": "^9.2.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	}
}
